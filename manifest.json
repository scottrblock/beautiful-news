{
  "manifest_version": 1,

  "name": "Better WorldStar",
  "description": "An extension to make WorldStar more usable",
  "icons": { "128": "icon.png" },
  "version": "0.1",
  "homepage_url": "https://github.com/scottrblock/beautiful-news",
  

  "content_scripts": [
    {
      "matches": ["*://*.worldstarhiphop.com/*/*"],
      "css": ["ws.css"],
      "js": ["ws.js"],
      "run_at": "document_end"
    }
  ]
}